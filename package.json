{
  "name": "blog",
  "version": "1.0.0",
  "description": "my blog",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack && webpack --env.server",
    "watch:client": "webpack-dev-server --env.dev",
    "watch:server": "webpack --env.dev --env.server --watch",
    "predev": "webpack --env.dev && webpack --env.dev --env.server",
    "dev": "concurrently --raw 'yarn watch:client' 'yarn watch:server' 'nodemon dist'",
    "start": "node dist",
    "lint": "eslint .",
    "coverage": "jest --coverage",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(gif|png|jpe?g|svg|woff|woff2)$": "<rootDir>/__mocks__/filemock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babili-webpack-plugin": "^0.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint": "^4.2.0",
    "fast-async": "^6.3.0",
    "file-loader": "^0.11.2",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "isomorphic-style-loader": "^3.0.0",
    "jest-css-modules": "^1.1.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "preact-render-spy": "^1.0.0-rc.7",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-stats-plugin": "^0.1.5"
  },
  "dependencies": {
    "express": "^4.15.3",
    "node-fetch": "^1.7.1",
    "preact": "^8.2.1",
    "preact-render-to-string": "^3.6.3",
    "preact-transition-group": "^1.1.1",
    "promise-polyfill": "^6.0.2",
    "unfetch": "^3.0.0"
  }
}
